{
  "version": 2,
  "builds": [
    {
      "src": "src/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/auth(.*)",
      "dest": "src/routes/authRoutes.js"
    },
    {
      "src": "/cart(.*)",
      "dest": "src/routes/cartRoutes.js"
    },
    {
      "src": "/checkout(.*)",
      "dest": "src/routes/orderRoutes.js"
    },
    {
      "src": "/products(.*)",
      "dest": "src/routes/productRoutes.js"
    }
  ]
}
